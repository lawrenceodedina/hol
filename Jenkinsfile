pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                sleep 5
            }
        }
        stage('build') {
            steps {
                echo 'Hello build'
                sleep 5
            }
        }
        stage('deploy') {
            steps {
                echo 'Hello deploy'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello Test'
            }
        }
    }
}
